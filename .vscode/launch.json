    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
            ]
        },
        {
            "name": "VISUALIZER TESTER - Plane",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/chess3d/visualizer.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceRoot}/chess3d",
            "env": {
                "PYTHONPATH": "${workspaceRoot}"
            }
        },
        {
            "name": "VISUALIZER TESTER - Earth",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/visualizer.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceRoot}/chess3d",
            "env": {
                "PYTHONPATH": "${workspaceRoot}"
            },
            "args": [
                "acbba_test_sats",
                "earth"
            ]
        },
        {
            "name": "VISUALIZER TEST - SAT SIM",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/chess3d/main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceRoot}/chess3d/",
            "env": {
                "PYTHONPATH": "${workspaceRoot}"
            },
            "args": "visualizer_test"
        },
        {
            "name": "TOY",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceRoot}",
            "env": {
                "PYTHONPATH": "${workspaceRoot}/chess3d"
            },
            "args": "toy"
        },
        {
            "name": "TOY RANDOM",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceRoot}",
            "env": {
                "PYTHONPATH": "${workspaceRoot}/chess3d"
            },
            "args": "toy_random"
        }
